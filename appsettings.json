{
  "Serilog" : {
    "Using" : [],
    "MinimumLevel" : {
      "Default" : "Information",
      "Override" : {
        "Microsoft" : "Warning",
        "System" : "Warning"
      }
    },
    "Enrich" : [ "FromLogContext" , "WithMachineName", "WithProcessId", "WithThreadId"],
    "WriteTo" : [
      {"Name" : "Console"},
      {
        "Name" : "SQLite",
        "Args" : {
          "sqliteDbPath" : ".\\Logs\\log.db",
          "tableName" : "Logs"
        }
      }
    ]
},
"AllowedHosts": "*",
    "ConnectionStrings": {
      "SaleDatabase": "Data Source=.\\FileBd\\mybd.sqlite;"
    }
}
